# Private zsh configuration
# This file should be created at ~/.private/.zshrc

# Workspace directory for development projects
export WORKSPACE="$HOME/workspace"

# GitHub configuration (for gh CLI and git operations)
export GITHUB_TOKEN="your-github-token-here"
export GITHUB_EMAIL="your-email@example.com"

# Tmux default path
export TMUX_DEFAULT_PATH="$WORKSPACE"

# Docker configuration (if using Podman or remote Docker)
# export DOCKER_CERT_PATH=""
# export DOCKER_HOST=""
# export DOCKER_TLS_VERIFY=""

# Add any other machine-specific or private environment variables here
